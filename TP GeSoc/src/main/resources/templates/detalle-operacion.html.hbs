{{# partial "titulo" }}
    <title>Gesoc - Detalle de Operacion</title>
{{/partial}}

{{# partial "contenido" }}
    <section class="fondo-gris">
        <div>
            {{#compra}}
            <span>Id:</span><span>{{id}}</span>
            {{/compra}}
        </div>
        <div>
            <span>Items:</span>
            <ul>
            {{#items}}
                <li>{{descripcion}}</li>
            {{/items}}
            </ul>
        </div>
        <div>
            {{#documentoComercial}}
            <span>Documento Comercial:</span><span>{{id}}</span>
            {{/documentoComercial}}
        </div>
        <div>
            {{#compra}}
            <span>Fecha de Operacion:</span><span>{{fechaOperacion}}</span>
            {{/compra}}
        </div>
        <div>
            {{#compra}}
            <span>Medio de Pago:</span><span>{{medio}}</span>
            {{/compra}}
        </div>
        <div>
            {{#proveedor}}
            <span>Proveedor:</span><span>{{razonSocial}}</span>
            {{/proveedor}}
        </div>
        <div>
            <span>Presupuestos:</span>
            <ul>
                {{#presupuestos}}
                    <li>{{id}}</li>
                {{/presupuestos}}
            </ul>
        </div>
        <div>
            <!--Ver que onda con esto porke no puedo hacer display del objeto Presupuesto /-->
            {{#presupuestoElegido}}
            <span>Presupuesto Elegido:</span><span>{{id}}</span>
            {{/presupuestoElegido}}
        </div>
        <div>
            <span>Revisores:</span>
            <ul>
                {{#revisores}}
                    <li>{{username}}</li>
                {{/revisores}}
            </ul>
        </div>
        <div>
            <span>Validaciones:</span>
            <ul>
                {{#validaciones}}
                    <li>{{id}}</li>
                {{/validaciones}}
            </ul>
        </div>
        <div>
            <span>Etiquetas:</span>
            <ul>
                {{#etiquetas}}
                    <li>{{texto}}</li>
                {{/etiquetas}}
            </ul>
        </div>
        <div>
            {{#compra}}
            <span>Presupuestos Minimos:</span><span>{{presupuestosMinimos}}</span>
            {{/compra}}
        </div>
        <div>
            {{#compra}}
            <span>Estado de la Operacion:</span><span>{{estado}}</span>
            {{/compra}}
        </div>
    </section>
{{/partial}}

{{> layout.html.hbs }}